#include <iostream>
using namespace std;
int main()
{
	setlocale(LC_ALL, "ru");


	int a, b, c, d, sw = 0;

	cout << "Введите по очереди значения а, b, c, d : ";
	cin >> a >> b >> c >> d;
	for (int i = 10000; i < 100000; i++)
	{
		if ((i % a == b) && (i % c == d))
                {
                sw = 1;
                cout << i << " ";
                }
			
	}
	cout << endl;
        if (sw = 0) 
          return -1;
	return 1;
}
