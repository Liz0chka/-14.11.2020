#include <iostream>

using namespace std;

int main()
{
	const int n = 10;
	int i, count = 0;
	double res = 0;
	int arr[n];
	
	for (i = 0; i < n; i++)
	{
		cin >> arr[i];
		if (arr[i] % 2 == 1)
		{
			res += arr[i];
			count++;
		}
	}
	if (count = 0) 
		cout << "NO";
	else
	{
		res = res / (double)count;
		cout << "Res = " << res;
	}

	return 0;
}
